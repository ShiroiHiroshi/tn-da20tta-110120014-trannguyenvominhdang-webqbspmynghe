@model IEnumerable<WebsiteQuangBaMyNghe.Models.EF.DanhMuc>
@if (Model != null && Model.Any())
{
    <ul class="sidebar_categories">
        @foreach (var item in Model)
        {
            if (ViewBag.CateId == item.MaDanhMuc)
            {
                <li class="active"><a href="/danh-muc-san-pham/@item.Alias-@item.MaDanhMuc"><span><i class="fa fa-angle-double-right" aria-hidden="true"></i></span>@item.TenDanhMuc</a></li>
            }
            else
            {
                <li><a href="/danh-muc-san-pham/@item.Alias-@item.MaDanhMuc">@item.TenDanhMuc</a></li>
            }
        }
    </ul>
}
